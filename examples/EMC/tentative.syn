`$`: unit;
`$/MWh` ::= `$`/`MWh`;

MNN: set;
GRF: set;
m_from_GRF: GRF -> MNN;

SZ: GRF -> `MWh`;

SCU: P'(GRF);

CSZ ::= 5 * `MWh`;

PCU ::= { g: GRF \' SCU | SZ(g) >= CSZ };

OrderedPCU ::= order(PCU, decreasing(p->SZ(p)));

z ::= sequence(OrderedPCU);

m_from_index: n: N0'(..length(z)-1) -> MNN ::= m_fromGRF(z(n));
